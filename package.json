{
  "name": "yold",
  "description": "A library for working with naive ISO8061 dates",
  "version": "1.0.0",
  "scripts": {
    "build": "tsup",
    "test": "npx vitest"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "date",
    "time",
    "iso8601",
    "naive",
    "naive date"
  ],
  "author": "liam-tait",
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "tsup": "^8.2.2",
    "typescript": "^5.5.3",
    "vitest": "^2.0.3"
  },
  "tsup": {
    "format": [
      "esm",
      "cjs"
    ],
    "entry": [
      "src/index.ts"
    ],
    "dts": true,
    "splitting": false,
    "sourcemap": true,
    "clean": true
  }
}
