{
	"name": "yold",
	"description": "A library for working with naive ISO8061 dates",
	"version": "1.0.0",
	"scripts": {
		"build": "tsup",
		"test": "tsx --test './test/**/*.test.*'"
	},
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"keywords": ["date", "time", "iso8601", "naive", "naive date"],
	"author": "liam-tait",
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@types/node": "^20.14.11",
		"tsup": "^8.2.2",
		"tsx": "^4.16.2",
		"typescript": "^5.5.3"
	},
	"engines": { "node": ">= 21.5" },
	"tsup": {
		"format": ["esm", "cjs"],
		"entry": ["src/index.ts"],
		"dts": true,
		"splitting": false,
		"sourcemap": true,
		"clean": true
	}
}
